{
  "name": "kickstart-next",
  "version": "1.0.3",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/contentstack/kickstart-next"
  },
  "description": "Contentstack Kickstarts are the minimum amount of code needed to connect to Contentstack.",
  "author": "Tim Benniks <tim.benniks@contentstack.com>",
  "annotation": "Lo Etheridge <lo.etheridge@contentstack.com>",
  "homepage": "https://contentstack.com",
  "license": "MIT",
  "scripts": {
    "tsgen": "csdx tsgen -a CSDT -o \"./lib/generated.d.ts\" -p \"I\" --branch=main --no-doc --include-system-fields --include-editable-tags",
    "gen-config": "tsx ./scripts/generate-component-mapper.ts",
    "gen-config:watch": "tsx ./scripts/generate-component-mapper.ts --watch",
    "dev": "concurrently \"npm run gen-config:watch\" \"next dev\"",
    "build": "npm run gen-config && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "prod": "npm run build && npm run start"
  },
  "dependencies": {
    "@contentstack/delivery-sdk": "^4.7.0",
    "@contentstack/json-rte-serializer": "^3.0.3",
    "@contentstack/live-preview-utils": "^3.4.0",
    "@contentstack/utils": "^1.4.4",
    "@hookform/resolvers": "^5.2.1",
    "@lottiefiles/dotlottie-react": "^0.15.2",
    "@timbenniks/contentstack-endpoints": "^1.0.10",
    "@types/flatpickr": "^3.0.2",
    "@types/react-google-recaptcha": "^2.1.9",
    "clsx": "^2.1.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "flatpickr": "^4.6.13",
    "googleapis": "^159.0.0",
    "isomorphic-dompurify": "^2.26.0",
    "next": "^15.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.62.0",
    "swiper": "^11.2.10",
    "tailwind-merge": "^3.3.1",
    "yup": "^1.7.0",
    "zoomist": "^2.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@next/eslint-plugin-next": "^15.5.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "autoprefixer": "^10.4.17",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "postcss": "^8.5.3",
    "prettier": "^2.8.3",
    "tailwindcss": "^4.1.7",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.41.0"
  }
}
